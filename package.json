{
  "name": "tikspark",
  "version": "2.2.17",
  "description": "TikSpark - Detector de Regalos de TikTok",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zfloresdelcampo/TikSpark.git"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "publish": "electron-builder --publish always"
  },
  "author": "JaddelTwo",
  "license": "ISC",
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "electron-updater": "^6.2.1",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "tiktok-live-connector": "^2.1.0",
    "winreg": "^1.2.5",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.tikspark.app",
    "productName": "TikSpark",
    "publish": {
      "provider": "github"
    },
    "asarUnpack": [
      "keystroke.au3"
    ],
    "files": [
      "**/*",
      "!dist/**/*",
      "!node_modules/.cache/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
